{
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": ["ie 6"]
            },
            "modules": false
          }
        ]
      ],
      "plugins": ["external-helpers"]
    },
    "production": {
      "presets": [
        "minify",
        [
          "env",
          {
            "targets": {
              "browsers": ["ie 6"]
            },
            "modules": false
          }
        ]
      ],
      "plugins": ["external-helpers"],
      "comments": false
    },
    "escape": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": ["ie 6"]
            },
            "modules": false
          }
        ]
      ],
      "plugins": ["./escape-js-html", "external-helpers"]
    },
    "escapemin": {
      "presets": [
        "minify",
        [
          "env",
          {
            "targets": {
              "browsers": ["ie 6"]
            },
            "modules": false
          }
        ]
      ],
      "plugins": ["external-helpers"]
    }
  }
}
